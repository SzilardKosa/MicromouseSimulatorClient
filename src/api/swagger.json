  "components": {
    "schemas": {
      "AlgorithmDTO": {
        "required": ["codeText", "language", "name"],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string"
          },
          "language": {
            "pattern": "^Python$|^C$|^C\\+\\+$",
            "type": "string"
          },
          "codeText": {
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CellWalls": {
        "required": ["bottom", "left"],
        "type": "object",
        "properties": {
          "bottom": {
            "type": "boolean"
          },
          "left": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "Coordinate": {
        "required": ["x", "y"],
        "type": "object",
        "properties": {
          "x": {
            "type": "integer",
            "format": "int32"
          },
          "y": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "GoalAreaDTO": {
        "required": ["cell1", "cell2"],
        "type": "object",
        "properties": {
          "cell1": {
            "$ref": "#/components/schemas/Coordinate"
          },
          "cell2": {
            "$ref": "#/components/schemas/Coordinate"
          }
        },
        "additionalProperties": false
      },
      "MazeDTO": {
        "required": ["goalArea", "height", "isFullSize", "name", "walls", "width"],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string"
          },
          "isFullSize": {
            "type": "boolean"
          },
          "width": {
            "maximum": 32,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "height": {
            "maximum": 32,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "goalArea": {
            "$ref": "#/components/schemas/GoalAreaDTO"
          },
          "walls": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "$ref": "#/components/schemas/CellWalls"
              }
            }
          }
        },
        "additionalProperties": false
      },
      "MouseDTO": {
        "required": ["isFullSize", "name"],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string"
          },
          "isFullSize": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "SimulationDTO": {
        "required": ["name"],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string"
          },
          "algorithmId": {
            "type": "string",
            "nullable": true
          },
          "mazeId": {
            "type": "string",
            "nullable": true
          },
          "mouseId": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SimulationExpandedDTO": {
        "required": ["name"],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string"
          },
          "algorithmId": {
            "type": "string",
            "nullable": true
          },
          "mazeId": {
            "type": "string",
            "nullable": true
          },
          "mouseId": {
            "type": "string",
            "nullable": true
          },
          "algorithm": {
            "$ref": "#/components/schemas/AlgorithmDTO"
          },
          "maze": {
            "$ref": "#/components/schemas/MazeDTO"
          },
          "mouse": {
            "$ref": "#/components/schemas/MouseDTO"
          }
        },
        "additionalProperties": false
      },
      "SimulationResultDTO": {
        "type": "object",
        "properties": {
          "error": {
            "type": "string",
            "nullable": true
          },
          "simulation": {
            "$ref": "#/components/schemas/SimulationExpandedDTO"
          },
          "history": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
